{
  "metadata": {
    "title": "Data Analysis Notebook",
    "description": "Template for data analysis and visualization projects",
    "author": "AgentWorkbook Team",
    "version": "1.0.0",
    "tags": ["data-science", "analysis", "python", "visualization"]
  },
  "arguments": {
    "datasetName": {
      "type": "string",
      "label": "Dataset Name",
      "description": "Name of the dataset you'll be analyzing",
      "required": true,
      "defaultValue": "sample_data"
    },
    "analysisType": {
      "type": "select",
      "label": "Analysis Type",
      "description": "Type of analysis to perform",
      "options": ["Exploratory Data Analysis", "Statistical Analysis", "Machine Learning", "Time Series Analysis"],
      "defaultValue": "Exploratory Data Analysis"
    },
    "sampleSize": {
      "type": "int",
      "label": "Sample Size",
      "description": "Number of samples to analyze (if applicable)",
      "defaultValue": 1000
    },
    "includeVisualization": {
      "type": "bool",
      "label": "Include Visualization",
      "description": "Add visualization cells with matplotlib/seaborn",
      "defaultValue": true
    }
  },
  "template": {
